      "properties": {
        "product": {
          "$ref": "#/$defs/Product",
          "description": "Plain language description of the nature of a goods item sufficient to identify it for customs, statistical or transport purposes."
        },
        "sku": {
          "type": "string",
          "description": ""
        },
        "itemCount": {
          "type": "number",
