        },
        "grossWeight": {
          "$ref": "#/$defs/Quantity",
          "description": ""
        },
        "productPrice": {
          "$ref": "#/$defs/MonetaryAmount",
          "description": ""
        },
        "commodity": {
          "example": 950629,
          "type": "string",
          "description": "Code specifying a type of goods for Customs, transport or statistical purposes (generic term)."
        },
        "amount": {
          "$ref": "#/$defs/MonetaryAmount",
          "description": ""
        }
      },
